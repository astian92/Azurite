@model IEnumerable<Azurite.Store.Wrappers.CategoryW>

@{
    ViewBag.Title = "Collections";
}

@foreach (var item in Model) {
    <div class="col-xs-6 col-sm-4 col-md-3 col-lg-2">
        <div class="thumbnail">
            <a href="/Categories/Index/@item.Id" class="categoryThumb">
                <div class="imageWrapper">
                    <img src="http://localhost:8696/cdn/files/@item.ImagePath" alt="feature-collection-image">
                </div>
                <div class="caption">
                    <h4>@item.Name</h4>
                </div>
                <div class="clearfix"></div>
            </a>
        </div>
    </div>
}
